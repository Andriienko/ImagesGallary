@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts
{
    @Scripts.Render("~/bundles/scripts/home")
}
<input type="hidden" id="userName" value="@ViewData["UserName"]" />
<div id="users" class="section panel panel-primary">
        <div class="panel-heading">Users</div>
        <div class="panel-body">
            <table class="table table-striped table-condensed">
                <thead>
                    <tr id="head"><th>User Name</th><th>Email</th><th></th><th></th><th></th></tr>
                </thead>
                <tbody data-bind="foreach: viewModel.users">
                    <tr>
                        <td data-bind="text:UserName"></td>
                        <td data-bind="text: Email"></td>
                        <td>
                            <button class="btn btn-xs btn-primary"
                                    data-bind="click:viewModel.friendHandler">
                                Add To Friends
                            </button>
                        </td>
                        <td class="profile-btn"> <a href="#" class="btn btn-info btn-md" role="button" data-bind="click:viewModel.profileHandler">Profile</a></td>
                        <td><a href="#" class="btn btn-info" role="button" data-bind="click:viewModel.galleryHandler">Gallery</a></td>
</tr>
                </tbody>
            </table>
        </div>
    </div>
    


